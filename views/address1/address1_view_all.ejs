<!DOCTYPE html>
<html>

<head>
    <title> Address - View All </title>
</head>
    <body>
        <h1> Addresses </h1>

        <a href="/address1/add">Add New Address</a> <br />

        <table border = "1">
            <tr>
                <th>ID </th>
                <th>Street </th>
                <th>City </th>
                <th>State </th>
                <th>Zip </th>
            </tr>
            <% for(var i = 0; i < result.length; i++){ %>
                <tr>
                    <td><%= result[i].address_id %></td>
                    <td><%= result[i].street %></td>
                    <td><%= result[i].city %></td>
                    <td><%= result[i].state %></td>
                    <td><%= result[i].zip_code %></td>
                </tr>
            <% } %>
        </table>

    </body>
</html>